{
  "name": "bustawin-vite",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "tsx ./server.ts",
    "dev-debug": "export DEBUG=vite:* && npm run dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "restart-docker": "docker-compose down --remove-orphans -v && docker-compose up --build --detach",
    "start": "cross-env NODE_ENV=production tsx ./server.ts",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@pintora/cli": "^0.7.6",
    "@remix-run/express": "^2.9.2",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.9.2",
    "@types/plotly.js": "^2.29.4",
    "bootstrap": "^5.3.3",
    "change-case": "^5.4.4",
    "compression": "^1.7.4",
    "esbuild": "^0.20.2",
    "express": "^4.19.2",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "isbot": "^4.4.0",
    "iterated": "^0.2.2",
    "mdx-bundler": "^10.0.2",
    "morgan": "^1.10.0",
    "plotly.js-basic-dist": "^2.32.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-plotly.js": "^2.6.0",
    "react-top-loading-bar": "^2.3.1",
    "rehype-highlight": "^7.0.0",
    "rehype-mdx-code-props": "^3.0.1",
    "remark-heading-id": "^1.0.1",
    "remark-mdx-images": "^3.0.0",
    "remark-mdx-toc": "^0.3.1",
    "remix-utils": "^7.6.0",
    "tiny-invariant": "^1.3.3",
    "@remix-run/dev": "^2.9.2",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "sass": "^1.77.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.4.5",
    "tsx": "^4.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "bootstrap-icons": "^1.11.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "ts-jest": "^29.1.3",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": ".test.ts$",
    "moduleNameMapper": {
      "^@src$": "<rootDir>/src.server",
      "^@src/(.*)$": "<rootDir>/src.server/$1",
      "^@ui$": "<rootDir>/ui",
      "^@ui/(.*)$": "<rootDir>/ui/$1"
    }
  }
}
